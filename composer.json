{
    "name": "fidelize/jwt-auth-multiple-keys",
    "description": "Add multiple keychain support to tymon/jwt-auth package",
    "keywords": [
        "auth",
        "authentication",
        "json web token",
        "jwt",
        "laravel"
    ],
    "homepage": "https://github.com/fidelize/jwt-auth-multiple-keys",
    "support": {
        "issues": "https://github.com/fidelize/jwt-auth-multiple-keys/issues",
        "source": "https://github.com/fidelize/jwt-auth-multiple-keys"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Alan Michel Willms Quinot",
            "email": "dyulax@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "lcobucci/jwt": "^3.2",
        "laravel/framework": ">=5.0",
        "tymon/jwt-auth": "0.5.*",
        "guzzlehttp/guzzle": "^6.5"
    },
    "require-dev": {
        "mockery/mockery": "1.3.*",
        "phpunit/phpunit": "~6"
    },
    "autoload": {
        "psr-4": {
            "Fidelize\\JWTAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fidelize\\JWTAuth\\Test\\": "tests/"
        }
    }
}
